enum Permission {
  USER
  OWNER
}

type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  resetToken: String
  resetTokenExpiry: Float
  permissions: [Permission]
}

type Game {
  id: ID! @unique
  players: Int!
  city: String!
  street: String!
  date: String!
  hour: String!
  description: String!
  # user: User!
}
